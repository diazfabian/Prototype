    <validationRules>
        <fullName>ChangedStatusEstimated</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Status__c ,&apos;Estimated&apos;) ,
  ISBLANK(Story_Point__c)  
)</errorConditionFormula>
        <errorMessage>If the status is Estimated you must have a story point.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoChangedEstimated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(Status__c,&apos;Estimated&apos;), 
ISPICKVAL(Status__c,&apos;Pending Demo&apos;),
ISPICKVAL(Status__c,&apos;Dev In Progress&apos;),
ISPICKVAL(Status__c,&apos;Pending QA&apos;),
ISPICKVAL(Status__c,&apos;QA In Progress&apos;)
),
ISCHANGED( Story_Point__c ) )</errorConditionFormula>
        <errorMessage>You can not update the story point, because it has been estimated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NochangedPoint</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c, &apos;Done&apos;),
ISCHANGED(Story_Point__c)
)</errorConditionFormula>
        <errorMessage>You can not change the  Storie Point</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Permission_For_Team_Member_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Status__c ,&apos;Done&apos;),  $Profile.Name = &apos;TMember&apos;)</errorConditionFormula>
        <errorMessage>You can not update the status to done, because you have the necessary permission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValuesPO</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR
(
 ISPICKVAL(Status__c, &apos;Dev In Progress&apos;),
ISPICKVAL(Status__c, &apos;Pending QA&apos;),
ISPICKVAL(Status__c, &apos;QA In Progress&apos;),
 ISPICKVAL(Status__c, &apos;Estimated&apos;)) , 
$Profile.Name = &apos;Product Owner&apos;)</errorConditionFormula>
        <errorMessage>You can only select the following states: 
Done,
Rejected,
Pending Demo
and Pending Estimation</errorMessage>
    </validationRules>
